{
  "name": "coobook",
  "version": "1.0.0",
  "description": "Application responsible for store, create and manage recipes.",
  "main": "",
  "scripts": {
    "deploy-application": "sh ./deploy.sh",
    "build-backend": "webpack --config ./api/webpack.config.js",
    "watch-build-backend": "webpack --watch=true --config ./api/webpack.config.js",
    "start-backend": "nodemon ./api/dist/main.js localhost 8080",
    "build-frontend": "webpack --config ./frontend/webpack.config.js",
    "start-frontend": "webpack-dev-server --config ./frontend/webpack.config.js",
    "build": "concurrently --kill-others \"npm run build-backend\" \"npm run build-frontend\"",
    "dev": "concurrently --kill-others \"npm run watch-build-backend\" \"npm run start-backend\" \"npm run start-frontend\"",
    "build-cli": "webpack --config ./api/webpack.config.js --entry ./api/cli/index.js -o ./api/dist/cli.js\n",
    "build-database": "node ./api/dist/cli.js deploy --file=schema.sql",
    "push-data": "node ./api/dist/cli.js createData",
    "lint-frontend": "eslint ./frontend/**/*.js",
    "fix-lint-frontend": "eslint ./frontend/**/*.js --fix"
  },
  "pre-commit": [
    "lint-frontend"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Susniak/react_cookbook.git"
  },
  "author": "Marek Susniak",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Susniak/react_cookbook/issues"
  },
  "homepage": "https://github.com/Susniak/react_cookbook#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.6",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "express": "^4.17.1",
    "html-webpack-plugin": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-redux": "^7.1.1",
    "react-rich-text-editor": "^2.1.2",
    "react-router-dom": "^5.1.2",
    "readline": "^1.3.0",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.1",
    "sass-loader": "^8.0.2",
    "sha1": "^1.1.1",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "react-dropzone": "^10.2.1",
    "react-quill": "^1.3.3",
    "reset-css": "^5.0.1"
  }
}
